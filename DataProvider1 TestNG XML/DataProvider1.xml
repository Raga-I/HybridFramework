<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<groups>
		<run>
			<include name="Smoke"></include>
		</run>
	</groups>
  <test thread-count="5" name="Positive test case validation">
    <classes>
      <class name="testCasePackage.TestCases"/>
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="Negative test case validation">
    <classes>
      <class name="testCasePackage.NegativeTestCase"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->

